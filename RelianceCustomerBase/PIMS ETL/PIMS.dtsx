                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
        OriginalCustomerCode = rtrim(ltrim(CstmrCd.Key0)),
        EmailAddress         = rtrim(ltrim(EmlAdrs.Key0)),
        PhoneNumber         =left(replace(replace(replace(replace(rtrim(ltrim(PhoneAdrs.Key1)),'(',''),')',''),'-',''),' ',''),40),
		
       MailAddress         =left(rtrim(ltrim(Adrs.KEY0))+' '+rtrim(ltrim(Adrs.KEY1))+' '+replace(rtrim(ltrim(Adrs.KEY3)),' ',''),100),
	   
        SuppressTelephone    = PrvcyPrfrncs.Key1,
        SuppressPostal       = PrvcyPrfrncs.Key2,
        SuppressElectronic   = PrvcyPrfrncs.Key3
      
  
  FROM (select OwnerOID,Key1,Key2,Key3 from PersistentObjectStore  where ClassName = 'ue.domain.objects.PrivacyPreferences' and Deleted = 0 ) PrvcyPrfrncs
  LEFT OUTER JOIN PersistentObjectStore CstmrCd ON ( PrvcyPrfrncs.OwnerOID = CstmrCd.OwnerOID AND CstmrCd.ClassName = 'ue.domain.objects.BannerCustomerLink' )
  INNER JOIN PersistentObjectStore EmlAdrs ON ( PrvcyPrfrncs.OwnerOID = EmlAdrs.OwnerOID AND EmlAdrs.ClassName = 'ue.domain.objects.InternetEmailAddress' )
  INNER JOIN PersistentObjectStore PhoneAdrs ON ( PrvcyPrfrncs.OwnerOID = PhoneAdrs.OwnerOID AND PhoneAdrs.ClassName = 'ue.domain.objects.PhoneNumber' )
  INNER JOIN PersistentObjectStore Adrs ON ( PrvcyPrfrncs.OwnerOID = Adrs.OwnerOID AND Adrs.ClassName ='ue.domain.objects.Address')</property>
